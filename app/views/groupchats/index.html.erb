<p id="notice"><%= notice %></p>

<h1>Groupchats</h1>

<table>
  <thead>
    <tr>
      <th>Group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groupchats.each do |groupchat| %>
      <tr>
        <td><%= groupchat.group %></td>
        <td><%= link_to 'Show', groupchat %></td>
        <td><%= link_to 'Edit', edit_groupchat_path(groupchat) %></td>
        <td><%= link_to 'Destroy', groupchat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Groupchat', new_groupchat_path %>
